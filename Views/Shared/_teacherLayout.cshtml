<!-- js for banner section -->
<script src="~/Script/bgfader.js"></script>
<script>
    var myBgFader = $('.header').bgfader([
	'/images/banner1.jpg',
	'/images/banner2.jpg',
	'/images/banner3.jpg',
	'/images/banner4.jpg',
    ], {
        'timeout': 3000,
        'speed': 3000,
        'opacity': 0.4
    })
    myBgFader.start()
</script>
<!-- /js for banner section -->
<!-- js for parallax effect -->
<script src="~/Script/jarallax.js"></script>
<script type="text/javascript">
    /* init Jarallax */
    $('.jarallax').jarallax({
        speed: 0.5,
        imgWidth: 1366,
        imgHeight: 768
    })
</script>
<!-- /js for parallax effect -->
