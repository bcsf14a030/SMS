					<li><a href="/Home/contact" class="page-scroll" data-hover="Contact">Contact</a></li>
@*                    <li><a href="/Home/login_signup" class="page-scroll" data-hover="Login">Login</a></li>*@
                    @{
                        if (Session["typeo"] == "home" || Session["typeo"] == "student" || Session["typeo"] == "teacher" || Session["typeo"] == "admin")
